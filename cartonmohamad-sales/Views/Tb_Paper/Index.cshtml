@model IEnumerable<cartonmohamad_sales.Models.Tb_Paper>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Paper_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price_per_sheet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.shipping_cost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Paper_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.price_per_sheet)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.shipping_cost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
